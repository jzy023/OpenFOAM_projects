FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.liquid 0
    volScalarFieldValue T 375
);

regions
(
    boxToCell
    {
        box (0.00   0.00   0.00) (0.02  0.0025   0.01);
        fieldValues
        (
            volScalarFieldValue alpha.liquid 1
            volScalarFieldValue T 365
        );
    }
);



