FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];
// dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101300;

boundaryField
{
    movingWall
    {
        // type            fixedFluxPressure;
        // rho             rhok;
        // value           $internalField;
        
        type            totalPressure;
        p0              $internalField;
    }

    fixedWalls
    {
        type            fixedFluxPressure;
        value           $internalField;
        // rho             rhok;
    }

    frontAndBack
    {
        type            empty;
    }
}


