FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-20;

boundaryField
{
    bottom
    {
        type            fixedValue;
        value           uniform 1e-20;
    }

    top
    {
        type            fixedValue;
        value           uniform 1e-20;
    }

    left
    {
        type            fixedValue;
        value           uniform 1e-20;
    }

    right
    {
        type            fixedValue;
        value           uniform 1e-20;
    }

    frontAndBack
    {
        type            empty;
    }
}

