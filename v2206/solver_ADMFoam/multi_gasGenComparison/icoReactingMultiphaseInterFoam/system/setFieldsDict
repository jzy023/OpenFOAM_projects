FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.fluid    1.0
    volScalarFieldValue alpha.air      0.0 
);

regions
(
    boxToCell
    {
        box (0 -0.04 0) (1 0.00 1);
        fieldValues
        (
            volScalarFieldValue alpha.fluid     0.0
            volScalarFieldValue alpha.air       1.0  
        );
    }
);


