// [mandatory for general case]
mode            MesoSolid;

// Schmidt number
Sc              1;

// Vapour pressure of water [Pa]
Pvap            3130;

// External pressure [Pa]
Pext            101300;

// Benchmark case (for case study only)
// benchmark       true;
benchmark       false;

// Pipe resistance [m3 * s-1 * Pa-1]
// Rosen: 5e4 [m3 * d-1 * bar-1]
// benmark value with 3700 m3 total volume
// Kpip            0.5;
Kpip            1e-20;    

// Not benchmark case
// benchmark       false;
// corrected by volume of the actual geometry from blockMesh
// Kpip            2.0020037384e-15; 


// Optional Entries ---------------------------------------------------------

// Default pH value
pH              7.26;

// Gas constant [Pa * K-1 * (mol/m3)-1]
R               8.3145;

// Cation concentration [mol * m-3]
Scat            0.00;

// Anion concentration [mol * m-3]
San             5.20;

// TODO: remove
// Gas transfer rate coefficients [d-1]
// kLa             1e-16;
