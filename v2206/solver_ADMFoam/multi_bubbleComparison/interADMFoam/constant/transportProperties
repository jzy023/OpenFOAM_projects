FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// transportModel  Newtonian;

phases              (liquid gas);

sigma
{
    type            constant;
    sigma           0.07;
}

Prt                 0.7;

degassing
{
    // Henry's coefficient
    H               1e-20;

    // volume fraction threshold to determine interface for inter-phase mass transfer
    // 
    alphaInterface  0.1;

    // boundary walls that promotes inter-phase mass trnasfer
    walls
    {
        "fixedWalls";
    }

    // Diffusion coefficients for soluables and gaseous species
    DS              1e-6;
    DG              1e-6;

    // DEBUG
    // mDotTest        1e-12;
    mDotTest        0.1;
}

liquid
{
    transportModel  Newtonian;
    nu              1e-6;
    rho             1000;

    Cp              4.186e3;
    Cv              4.186e3;   // assume Cp for liquid
    kappa           0.6;
    hf              -2.4e6;
}

gas
{
    transportModel  Newtonian;
    nu              1.8e-08;
    rho             1.3;

    Cp              1.87e3;     // water gas
    Cv              1.41e3;     // Cv = Cp - R/w
    kappa           0.026;      // water gas
    hf              0;
}



