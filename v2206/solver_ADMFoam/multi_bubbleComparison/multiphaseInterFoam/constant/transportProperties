FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

phases
(
    liquid
    {
        transportModel Newtonian;
        nu             1e-06;
        rho            1000;
    }

    air
    {
        transportModel Newtonian;
        nu             1.8e-08;
        rho            1.3;
    }
);

sigmas
(
    (liquid air)         0.07
);



