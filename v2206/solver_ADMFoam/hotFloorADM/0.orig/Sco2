FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Sva;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0.00942;

boundaryField
{
    movingWall
    {
        type            fixedValue;
        value           uniform 0.00942;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    floor
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

