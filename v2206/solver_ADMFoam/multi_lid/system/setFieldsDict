FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.liquid   1.0
    volScalarFieldValue alpha.Gch4     0.0
    volScalarFieldValue alpha.Gh2      0.0
    volScalarFieldValue alpha.Gco2     0.0
);

regions
(
    sphereToCell
    {
        centre (0.05 0.01 -0.005);
        radius 0.006;
        fieldValues
        (
            volScalarFieldValue alpha.liquid    0
            volScalarFieldValue alpha.Gch4      0.5
            volScalarFieldValue alpha.Gh2       0.5
            volScalarFieldValue alpha.Gco2      0.0
        );
    }

    // boxToCell
    // {
    //     box (0  0.05  -0.001) (0.1  0.1  0.0);
