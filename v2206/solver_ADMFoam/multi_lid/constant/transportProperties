FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

// nu              0.00;
nu              1e-5;

// Thermal expansion coefficient
beta            3e-03;

// Reference temperature
TRef            300;

// Laminar Prandtl number
Pr              0.7;

// Turbulent Prandtl number
Prt             0.85;



// multiphase testing
phases
(
    Gh2
    {
        transportModel  Newtonian;
        nu              1.8e-8;
        rho             0.1;  // 0.083;
        isRhoField      false;
    }

    Gch4
    {
        transportModel  Newtonian;
        nu              1.8e-8;
        rho             1.3;  // 0.657;
        isRhoField      false;
    }

    // Gco2
    // {
    //     transportModel Newtonian;
    //     nu             1e-05;
    //     rho            1.98;
    // }

    liquid
    {
        transportModel Newtonian;
        nu             1e-04;
        rho            1000;
    }
);

sigmas
(
    
    (liquid Gh2) 0.07
    (liquid Gch4) 0.07
    (Gh2 Gch4) 0.00


    // (liquid Gco2) 0.07
    // (Gh2 Gch4) 0.07
    // (Gh2 Gco2) 0.07
    // (Gch4 Gco2) 0.07
);



