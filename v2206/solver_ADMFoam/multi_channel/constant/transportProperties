FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

// nu              0.00;
nu              1e-5;

// Thermal expansion coefficient
beta            3e-03;

// Reference temperature
TRef            300;

// Laminar Prandtl number
Pr              0.7;

// Turbulent Prandtl number
Prt             0.85;


// multiphase testing
phases
(
    Gh2
    {
        transportModel Newtonian;
        nu             1e-06;
        rho            0.1;
        // rho            0.083;
    }

    Gch4
    {
        transportModel Newtonian;
        nu             1e-06;
        rho            0.657;
    }

    // Gco2
    // {
    //     transportModel Newtonian;
    //     nu             1e-05;
    //     rho            1.98;
    // }

    fluid
    {
        transportModel Newtonian;
        nu             1e-06;
        rho            1000;
    }
);

sigmas
(   
    (fluid Gh2) 0.07
    (fluid Gch4) 0.07
    // (fluid Gco2) 0.07
    (Gh2 Gch4) 0.00
    // (Gh2 Gco2) 0.00
    // (Gch4 Gco2) 0.00
);



