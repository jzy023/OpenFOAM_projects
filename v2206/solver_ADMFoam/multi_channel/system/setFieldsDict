FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.fluid    0.0
    volScalarFieldValue alpha.Gch4     1.0
    volScalarFieldValue alpha.Gh2      0.0
    volScalarFieldValue alpha.Gco2     0.0
);

regions
(
    boxToCell
    {
        box (0 0 0) (1 0.12 1);
        fieldValues
        (
            volScalarFieldValue alpha.fluid    1.0
            volScalarFieldValue alpha.Gch4     0.0 // 1.0
            volScalarFieldValue alpha.Gh2      0.0 // 0.0
            volScalarFieldValue alpha.Gco2     0.0 // 0.0
        );
    }


    sphereToCell
    {
        centre (0.02 0.02 0.0001);
        radius 0.004;
        fieldValues
        (
            volScalarFieldValue alpha.fluid    0.0
            volScalarFieldValue alpha.Gch4     0.0
            volScalarFieldValue alpha.Gh2      1.0
            volScalarFieldValue alpha.Gco2     0.0
        );
    }
);


