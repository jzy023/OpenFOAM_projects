FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=64;scalar=64";
    class       volScalarField;
    location    "489600";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.19627;

boundaryField
{
    movingWall
    {
        type            fixedFluxPressure;
        gradient        nonuniform List<scalar> 4(3.49419e-10 7.16908e-11 2.7778e-10 1.81623e-10);
        value           uniform 1.19627;
    }
    fixedWalls
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           uniform 1.19627;
    }
    frontAndBack
    {
        type            empty;
    }
}


