FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Snh3;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

// internalField   uniform 0.001884; // <- (kmol N m-3) from Rosen

internalField   uniform 1e-12; // (mol N m-3)

boundaryField
{
    movingWall
    {
        type            zeroGradient;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

