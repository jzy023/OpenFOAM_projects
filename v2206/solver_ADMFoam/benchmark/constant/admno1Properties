FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      admno1Properties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Operation modes
mode            MesoSolid;

// Schmidt number
Sc              0.2;

// Gas constant [Pa * K-1 * (mol/m3)-1]
R               8.3145;

// Default pH value
pH              7.26;

// Cation concentration [mol * m-3]
Scat            0.00;

// Anion concentration [mol * m-3]
San             5.20;

// Vapour pressure of water [Pa]
Pvap            3130;

// Gas transfer rate coefficients [d-1]
// kLa             1e-16;
kLa             200;
// kLa             214.1143;

// Pipe resistance
// Rosen:  5e4  [m3 * d-1 * bar-1]
// Rosen:  0.5  [m3 * d-1 * Pa-1]
// User ----->  [m3 * d-1 * Pa-1]
// Kpip            0.5;
Kpip            1e-32; // [m3 * d-1 * Pa-1]

// External pressure [Pa]
Pext            101300;

// Volume fraction between gas and liquid phase
Vfrac           0.08824; // [300 / 3400]


// Benchmark case
benchmark     false;
// benchmark     true;






