
	// Mid lines ===============================================================================

	mid_00
	{
		type						sets;
		libs						("libsampling.so");
		timeStart					45;
		writeControl				$writeControl;
		writeInterval				0.5;
		interpolationScheme			cell;
		setFormat					csv;
		sets						(
			data
			{
				type				uniform;
				axis				xyz;
