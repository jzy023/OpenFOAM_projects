FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Xpr;

}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField	uniform 0.0842;

boundaryField
{
    allBoundary
    {
        type            zeroGradient;
    }

    top
    {
        type            zeroGradient;
    }

    GeoTank
    {
        type            zeroGradient;
    }

    GeoShaft
    {
        type            zeroGradient;
    }
}


