FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

// internalField   uniform     0.8436;

// internalField   uniform     0.2732;
// internalField   uniform     0.0486;
// internalField   uniform     0.03375;
// internalField   uniform     0.0216;
internalField   uniform     0.0084375;

boundaryField
{
    airfoil
    {
        type            kqRWallFunction;
        value           uniform 1e-12;
    }
    west
    {
        type            fixedValue;
        value           $internalField;
    }
    east
    {
	    type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    north
    {
	type		slip;
    }
    south
    {
	type		slip;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}
