FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// dimensions      [1 -1 -2 0 0 0 0]; // for compressible
dimensions      [0 2 -2 0 0 0 0];  // for incompressible

// internalField   uniform 1e5; 
internalField   uniform 0; 

boundaryField
{
    west
    {
        type            calculated;
        value           $internalField; 
    }
    east
    {
        type            calculated;
        value           $internalField; 
    }
    top
    {
        // type            slip;
        type            calculated;
        value           $internalField; 
    }
    ground
    {
        type            calculated;
        value           $internalField; 
    }
    north
    {
        type            calculated;
        value           $internalField; 
    }
    south
    {
        type            calculated;
        value           $internalField; 
    }
    buildings
    {
        type            calculated;
        value           $internalField; 
    }
}


