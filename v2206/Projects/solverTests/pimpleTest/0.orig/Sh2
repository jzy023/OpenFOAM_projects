FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Sh2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// dimensions      [1 -3 0 0 0 0 0];
dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1.0; // 2.5055e-7;

boundaryField
{
    movingWall
    {
        type            zeroGradient;
        // type            fixedValue;
        // value           uniform 2.0;
    }

    fixedWalls
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

