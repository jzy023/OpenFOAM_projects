FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Sh2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// dimensions      [1 -3 0 0 0 0 0];
dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1; // 2.5055e-7;

boundaryField
{
    up
    {
        type            cyclic;
    }

    down
    {
        type            cyclic;
    }

    right
    {
        type            cyclic;
    }

    left
    {
        type            cyclic;
    }

    frontAndBack
    {
        type            empty;
    }
}

