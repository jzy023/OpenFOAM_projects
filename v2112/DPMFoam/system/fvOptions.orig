FoamFile
{
	version 	2.0;
	format		ascii;
	class 		dictionary;
	location	"system";
	object 		fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
momentumSource
{
    type    		meanVelocityForce;
    active  		yes;
    selectionMode   all;
    fields          (U);
    Ubar            (17.55 0 0);
    relaxation		0.8;
}
